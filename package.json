  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^9.3.12",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^16.0.0",
    "@types/secure-random": "^1.1.3",
    "@types/supertest": "^2.0.11",
    "@types/unidecode": "^0.1.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^27.2.5",
    "lint-staged": "^13.2.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps",
      "<rootDir>/libs"
    ],
    "moduleNameMapper": {
      "@app/common": "<rootDir>/libs/common/src",
      "@app/contracts": "<rootDir>/libs/contracts/src",
      "@app/base": "<rootDir>/libs/base/src",
      "@app/apm": "<rootDir>/libs/apm/src",
      "@app/apm/*": "<rootDir>/libs/apm/src/*",
      "@app/health": "<rootDir>/libs/health/src",
      "@app/health/*": "<rootDir>/libs/health/src/*",
      "@app/testing": "<rootDir>/libs/testing/src",
      "@app/testing/*": "<rootDir>/libs/testing/src/*"
    }
  }
}
